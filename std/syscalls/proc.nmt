func syscall_exit(code @int) {
    asm {
        "mov rax, 60"
        "mov edi, %code"
        "syscall"
    }
}

func syscall_alarm(secound @u32) {
    asm {
        "mov rax, 37"
        "mov rid, %secound"
        "syscall"
    }
}

func syscall_kill(pid @int, intSig @int) {
    asm {
        "mov rax, 62"
        "mov rdi, %pid"
        "mov rdi, %intSig"
        "syscall"
    }
}

